
# Customs
alias h="history"

# Find location of largest files (GBs only)
alias chksize="sudo du -h -d 1 / | grep '[0-9]\+G'"

# ReMount the sandisk /usr/src
alias src="sh ~/App/remount_src.sh"

# Python alias
alias py="python3"

# Trim file systems
alias trim="sudo fstrim -a -v"

